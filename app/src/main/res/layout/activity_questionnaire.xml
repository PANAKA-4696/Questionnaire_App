<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".questionnaire.Questionnaire">

    <TextView
        android:id="@+id/tvQuestionnaireTitle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:text="@string/tv_questionnaire_title"
        android:textSize="24sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tvQuestionnaireNowDayTime"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tvQuestionnaireDayTime"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        android:text="@string/tv_questionnaire_daytime"
        app:layout_constraintEnd_toStartOf="@+id/tvQuestionnaireNowDayTime"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tvQuestionnaireKana"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="8dp"
        android:text="@string/tv_questionnaire_kana"
        app:layout_constraintEnd_toStartOf="@+id/tvQuestionnairePutKana"
        app:layout_constraintTop_toBottomOf="@+id/tvQuestionnaireTitle" />

    <TextView
        android:id="@+id/tvQuestionnairePutKana"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="32dp"
        app:layout_constraintEnd_toStartOf="@+id/tvQuestionnaireOld"
        app:layout_constraintTop_toBottomOf="@+id/tvQuestionnaireTitle" />

    <TextView
        android:id="@+id/tvQuestionnaireName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        android:text="@string/tv_questionnaire_name"
        app:layout_constraintEnd_toStartOf="@+id/tvQuestionnairePutName"
        app:layout_constraintTop_toBottomOf="@+id/tvQuestionnaireKana" />

    <TextView
        android:id="@+id/tvQuestionnairePutName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="32dp"
        app:layout_constraintEnd_toStartOf="@+id/tvQuestionnaireOld"
        app:layout_constraintTop_toBottomOf="@+id/tvQuestionnairePutKana" />

    <TextView
        android:id="@+id/tvQuestionnaireOld"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="8dp"
        android:text="@string/tv_questionnaire_old"
        app:layout_constraintEnd_toStartOf="@+id/tvQuestionnairePutOld"
        app:layout_constraintTop_toBottomOf="@+id/tvQuestionnaireTitle" />

    <TextView
        android:id="@+id/tvQuestionnairePutOld"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="8dp"
        app:layout_constraintEnd_toStartOf="@+id/tvQuestionnaireAge"
        app:layout_constraintTop_toBottomOf="@+id/tvQuestionnaireTitle" />

    <TextView
        android:id="@+id/tvQuestionnaireAge"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="8dp"
        android:text="@string/tv_questionnaire_age"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tvQuestionnaireTitle" />

    <TextView
        android:id="@+id/tvQuestionnaireRole"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        android:text="@string/tv_questionnaire_role"
        app:layout_constraintEnd_toStartOf="@+id/tvQuestionnairePutRole"
        app:layout_constraintTop_toBottomOf="@+id/tvQuestionnaireOld" />

    <TextView
        android:id="@+id/tvQuestionnairePutRole"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tvQuestionnaireOld" />

    <TextView
        android:id="@+id/tvQuestionnaireQuestion"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="16dp"
        android:text="@string/tv_questionnaire_question"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tvQuestionnaireName" />

    <TextView
        android:id="@+id/tvQuestionnaireQuestion1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:text="@string/tv_questionnaire_question1"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tvQuestionnaireQuestion" />

    <EditText
        android:id="@+id/etQuestionnaireQuestion1"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        android:ems="10"
        android:inputType="text"
        android:textSize="12sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tvQuestionnaireQuestion1" />

    <TextView
        android:id="@+id/tvQuestionnaireQuestion2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:text="@string/tv_questionnaire_question2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/etQuestionnaireQuestion1" />

    <EditText
        android:id="@+id/etQuestionnaireQuestion2"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        android:ems="10"
        android:inputType="text"
        android:textSize="12sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tvQuestionnaireQuestion2" />

    <TextView
        android:id="@+id/tvQuestionnaireQuestion3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:text="@string/tv_questionnaire_question3"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/etQuestionnaireQuestion2" />

    <EditText
        android:id="@+id/etQuestionnaireQuestion3"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        android:ems="10"
        android:inputType="text"
        android:textSize="12sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tvQuestionnaireQuestion3" />

    <TextView
        android:id="@+id/tvQuestionnaireQuestion4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:text="@string/tv_questionnaire_question4"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/etQuestionnaireQuestion3" />

    <EditText
        android:id="@+id/etQuestionnaireQuestion4"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        android:ems="10"
        android:inputType="text"
        android:textSize="12sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tvQuestionnaireQuestion4" />

    <TextView
        android:id="@+id/tvQuestionnaireQuestion5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:text="@string/tv_questionnaire_question5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/etQuestionnaireQuestion4" />

    <EditText
        android:id="@+id/etQuestionnaireQuestion5"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        android:ems="10"
        android:inputType="text"
        android:textSize="12sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tvQuestionnaireQuestion5" />

    <TextView
        android:id="@+id/tvQuestionnaireQuestion6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:text="@string/tv_questionnaire_question6"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/etQuestionnaireQuestion5" />

    <RadioGroup
        android:id="@+id/rgQuestionnaireQuestion6"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:orientation="horizontal"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tvQuestionnaireQuestion6">

        <RadioButton
            android:id="@+id/rbQuestionnaireHomepage"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/rb_questionnaire_homepage"
            android:textSize="8sp" />

        <RadioButton
            android:id="@+id/rbQuestionnaireMouth"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/rb_questionnaire_mouth"
            android:textSize="8sp" />

        <RadioButton
            android:id="@+id/rbQuestionnaireWay"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/rb_questionnaire_way"
            android:textSize="8sp" />

        <RadioButton
            android:id="@+id/rbQuestionnaireTownpage"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/rb_questionnaire_townpage"
            android:textSize="8sp" />

        <RadioButton
            android:id="@+id/rbQuestionnaireIntroduce"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/rb_questionnaire_introduce"
            android:textSize="8sp" />

        <RadioButton
            android:id="@+id/rbQuestionnaireOther"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/rb_questionnaire_other"
            android:textSize="8sp" />

    </RadioGroup>

    <TextView
        android:id="@+id/tvQuestionnaireComment"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:text="@string/tv_questionnaire_comment"
        android:textSize="12sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/rgQuestionnaireQuestion6" />

    <EditText
        android:id="@+id/etQuestionnaireComment"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        android:ems="10"
        android:inputType="text"
        android:textSize="12sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/tvQuestionnaireComment"
        app:layout_constraintTop_toBottomOf="@+id/rgQuestionnaireQuestion6" />

    <Button
        android:id="@+id/btQuestionnaireNext"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="8dp"
        android:text="@string/bt_questionnaire_next"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />


</androidx.constraintlayout.widget.ConstraintLayout>